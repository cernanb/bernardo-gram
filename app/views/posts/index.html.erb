<div class="posts-wrapper row">
  <% @posts.each do |post| %>
    <div class="post">
      <div class="post-head">
        <div class="name">
          Cernan Bernardo
        </div>
      </div>
      <div class="image center-block">
        <%= image_tag post.image.url(:medium), class: 'img-responsive' %>
      </div>
      <p class="caption"><%= post.caption %></p>
    </div>
  <% end %>
</div>